Algoritmo culculodenota 
	definir n1,n2,n3,acu,prom como real;
	Escribir "ingresar la nota 1";
	Leer n1;
	Escribir "ingresar la nota 2";
	Leer n2;
	Escribir "ingresar la nota 3";
	Leer n3;
	acu=n1+n2+n3;
	prom=acu/3;
	Escribir "el promedio es" prom
	si prom  >=  7 Entonces
		Escribir "aprobado";
	sino 
		Escribir "reprobado";
		
	FinSi
FinAlgoritmo
